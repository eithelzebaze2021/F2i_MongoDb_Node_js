<% for(var i = 0; i < songs.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= songs[i].title %></td>
        <td><%= songs[i].url %></td>
        <td><%= songs[i].rating %></td>
        <td><%= songs[i].artist %></td>
        <td>
            <!--<a href="/modifier-song?id=<%= songs[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>-->
            <a class="btn border-shadow delete" data-id=<%= songs[i]._id%> >
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>